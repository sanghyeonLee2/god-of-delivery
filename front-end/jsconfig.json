{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@api/*": [
        "api/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@constants/*": [
        "constants/*"
      ],
      "@recoil/*": [
        "recoil/*"
      ],
      "@validation/*": [
        "validation/*"
      ],
      "@react-query/*": [
        "react-query/*"
      ]
    }
  },
  "include": [
    "src"
  ]
}
